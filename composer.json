{
    "name": "blackbullion/mandrill",
    "description": "Laravel 5 Mandrill API Wrapper",
    "keywords": ["mandrill", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@bluebaytravel.co.uk"
        }
    ],
    "require": {
        "php": ">=7.0",
        "graham-campbell/manager": "^4.2",
        "illuminate/contracts": "^5.7",
        "illuminate/support": "^5.7",
        "mandrill/mandrill": "1.0.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.2",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Blackbullion\\Mandrill\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blackbullion\\Tests\\Mandrill\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
